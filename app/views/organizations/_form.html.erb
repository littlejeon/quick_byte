<%= form_for (@organization) do |f| %>
<div class="name-field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div class="location-field">
  <%= f.label :location %>
  <%= f.text_field :location %>
</div>

<div class="logo-field">
  <%= f.label :logo_url %>
  <%= f.url_field :logo_url %>
</div>

<%= f.submit "Create Organization" %>
<% end %>



